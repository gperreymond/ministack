{
  "$schema": "https://raw.githubusercontent.com/jetify-com/devbox/0.13.7/.schema/devbox.schema.json",
  "packages": {
    "gomplate": "4.2.0",
    "go-task":  "3.40.1"
  },
  "shell": {
    "init_hook": [
      "echo 'Welcome to devbox!' > /dev/null"
    ],
    "scripts": {
      "ministack:dry-run": [
        "task ministack-dry-run -- \"$@\""
      ],
      "ministack:start": [
        "task ministack-start -- \"$@\""
      ],
      "ministack:stop": [
        "task ministack-stop -- \"$@\""
      ]
    }
  }
}
